      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "folder_clients": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::folder-client.folder-client",
      "mappedBy": "client"
    },
    "documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::document.document",
      "mappedBy": "client"
    }
  }
}
